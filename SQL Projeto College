CREATE DATABASE COLLEGE_SYSTEM;
USE COLLEGE_SYSTEM;

create table  TB_STUDENT(
ID BIGINT AUTO_INCREMENT PRIMARY KEY,
NAME VARCHAR(255),
ADDRESS VARCHAR(255),
NATIONALITY VARCHAR(128),
ID_COURSE BIGINT NULL
);

create table  TB_LOGIN_STUDENT(
ID BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY,
NAME_LOGIN VARCHAR(255),
PASSWORD CHAR(16),
FOREIGN KEY(ID) REFERENCES TB_STUDENT(ID)
);

create table  TB_LOGIN_ADMIN(
NAME_LOGIN VARCHAR(255),
PASSWORD CHAR(16)
);

create table  TB_COURSE(
ID BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY,
NAME VARCHAR(255),
PASSWORD CHAR(16)
);

INSERT INTO TB_LOGIN_ADMIN(NAME_LOGIN, PASSWORD) VALUES('admin', 'admin123');